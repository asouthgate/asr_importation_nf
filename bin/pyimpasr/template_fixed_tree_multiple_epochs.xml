<beast version="1.10.4">
	
        <taxa id="taxa">
!@TAXON_DATES_AND_LOCS@!
        </taxa>
	

	<!-- START Discrete Traits Model                                             -->

	<!-- general data type for discrete trait model, 'location'                  -->
	<generalDataType id="location.dataType">
		<!-- Number Of States = 2                                                    -->
		<state code="G"/>
		<state code="W"/>
	</generalDataType>


	<!-- Data pattern for discrete trait, 'loc'                                  -->
	<attributePatterns id="location.pattern" attribute="_loc">
		<taxa idref="taxa"/>
		<generalDataType idref="location.dataType"/>
	</attributePatterns>

	<!-- END Discrete Traits Model                                               -->

!@FIXED_TREE@!

	<!-- Generate a tree model                                                   -->
	<treeModel id="treeModel">
		<tree idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>

	<!-- Statistic for sum of the branch lengths of the tree (tree length)       -->
	<treeLengthStatistic id="treeLength">
		<treeModel idref="treeModel"/>
	</treeLengthStatistic>

	<!-- Statistic for time of most recent common ancestor of tree               -->
	<tmrcaStatistic id="age(root)" absolute="true">
		<treeModel idref="treeModel"/>
	</tmrcaStatistic>
	
    <parameter id="first.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="second.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="third.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="fourth.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="fifth.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="sixth.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="seventh.clock.rate" value="1.0" lower="0.0"/>
    <parameter id="eighth.clock.rate" value="1.0" lower="0.0"/>

<!--    <parameter id="third.clock.rate" value="1.0" lower="0.0"/> -->

    <rateEpochBranchRates id="epochClockRates">
        <epoch transitionTime="0.1">
            <parameter idref="first.clock.rate"/>
        </epoch>
        <epoch transitionTime="0.2">
            <parameter idref="second.clock.rate"/>
        </epoch>
        <epoch transitionTime="0.3">
            <parameter idref="third.clock.rate"/>
        </epoch>
        <epoch transitionTime="0.4">
            <parameter idref="fourth.clock.rate"/>
        </epoch>
        <epoch transitionTime="0.5">
            <parameter idref="fifth.clock.rate"/>
        </epoch>
        <epoch transitionTime="0.6">
            <parameter idref="sixth.clock.rate"/>
        </epoch>
        <epoch transitionTime="0.7">
            <parameter idref="seventh.clock.rate"/>
        </epoch>
        <rate>
            <parameter idref="eighth.clock.rate"/>
        </rate>
<!--        <rootHeight>
                        <parameter idref="treeModel.rootHeight"/>
        </rootHeight>
-->
    </rateEpochBranchRates>
<!--_____________-->

	

	<!-- START Discrete Traits Model                                             -->

	<!-- symmetric CTMC model for discrete state reconstructions                 -->
	<generalSubstitutionModel id="location.model">
		<generalDataType idref="location.dataType"/>
		<frequencies>
			<frequencyModel id="location.frequencyModel" normalize="true">
				<generalDataType idref="location.dataType"/>
				<frequencies>
					<parameter id="location.frequencies" dimension="2"/>
				</frequencies>
			</frequencyModel>
		</frequencies>

		<!-- rates and indicators                                                    -->
		<rates>
			<parameter id="location.rates" value="1.0" lower="0.0"/>
		</rates>
	</generalSubstitutionModel>
	<siteModel id="location.siteModel">
		<substitutionModel>
			<generalSubstitutionModel idref="location.model"/>
		</substitutionModel>
	</siteModel>

	<!-- Likelihood for tree given discrete trait data                           -->
	<ancestralTreeLikelihood id="location.treeLikelihood" stateTagName="location.states" useUniformization="true" saveCompleteHistory="false" logCompleteHistory="false">
		<attributePatterns idref="location.pattern"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="location.siteModel"/>
		<generalSubstitutionModel idref="location.model"/>
		<generalSubstitutionModel idref="location.model"/>
                <rateEpochBranchRates idref="epochClockRates"/>
	</ancestralTreeLikelihood>

	<!-- END Discrete Traits Model                                               -->


	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="log">
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="first.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="second.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="third.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="fourth.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="fifth.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="sixth.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="seventh.clock.rate"/>
                </scaleOperator>
                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="eighth.clock.rate"/>
                </scaleOperator>

<!--                <scaleOperator scaleFactor="0.75" weight="3">
                        <parameter idref="third.clock.rate"/>
                </scaleOperator> -->

		<scaleOperator scaleFactor="0.75" weight="15" scaleAllIndependently="true">
			<parameter idref="location.rates"/>
		</scaleOperator>
	</operators>
	

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="100000" autoOptimize="true" operatorAnalysis="fixed_tree_epoch.ops">
		<joint id="joint">
			<prior id="prior">
				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="first.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="second.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="third.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="fourth.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="fifth.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="sixth.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="seventh.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="eighth.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>

<!--				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="third.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior> -->

				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="location.frequencies"/>
				</uniformPrior>
				<cachedPrior>
					<gammaPrior shape="1.0" scale="1.0" offset="0.0">
						<parameter idref="location.rates"/>
					</gammaPrior>
					<parameter idref="location.rates"/>
				</cachedPrior>
				
			
                                <rateEpochBranchRates idref="epochClockRates"/>
			</prior>
			<likelihood id="likelihood">

				<!-- START Discrete Traits Model                                             -->
				<ancestralTreeLikelihood idref="location.treeLikelihood"/>

				<!-- END Discrete Traits Model                                               -->

			</likelihood>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="1000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="age(root)" sf="6" width="12">
				<tmrcaStatistic idref="age(root)"/>
			</column>
			<column label="first.clock.rate" sf="6" width="12">
				<parameter idref="first.clock.rate"/>
			</column>
			<column label="second.clock.rate" sf="6" width="12">
				<parameter idref="second.clock.rate"/>
			</column>

			<column label="third.clock.rate" sf="6" width="12">
				<parameter idref="third.clock.rate"/>
			</column>

			<column label="fourth.clock.rate" sf="6" width="12">
				<parameter idref="fourth.clock.rate"/>
			</column>

			<column label="fifth.clock.rate" sf="6" width="12">
				<parameter idref="fifth.clock.rate"/>
			</column>

			<column label="sixth.clock.rate" sf="6" width="12">
				<parameter idref="sixth.clock.rate"/>
			</column>

			<column label="seventh.clock.rate" sf="6" width="12">
				<parameter idref="seventh.clock.rate"/>
			</column>

			<column label="eighth.clock.rate" sf="6" width="12">
				<parameter idref="eighth.clock.rate"/>
			</column>

<!--			<column label="third.clock.rate" sf="6" width="12">
				<parameter idref="third.clock.rate"/> 
			</column> -->

			<!-- START Discrete Traits Model                                             -->

			<!-- END Discrete Traits Model                                               -->

		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1000" fileName="fixed_tree_epoch.log" overwrite="false">
			<joint idref="joint"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<tmrcaStatistic idref="age(root)"/>
			<treeLengthStatistic idref="treeLength"/>
			<parameter idref="first.clock.rate"/>
			<parameter idref="second.clock.rate"/>
			<parameter idref="third.clock.rate"/>
			<parameter idref="fourth.clock.rate"/>
			<parameter idref="fifth.clock.rate"/>
			<parameter idref="sixth.clock.rate"/>
			<parameter idref="seventh.clock.rate"/>
			<parameter idref="eighth.clock.rate"/>

<!--			<parameter idref="third.clock.rate"/> -->

			<!-- START Discrete Traits Model                                             -->
			<parameter idref="location.rates"/>

			<!-- END Discrete Traits Model                                               -->

<!--			<treeDataLikelihood idref="default.treeLikelihood"/> -->
<!--			<strictClockBranchRates idref="default.branchRates"/> -->

			<!-- START Discrete Traits Model                                             -->
			<ancestralTreeLikelihood idref="location.treeLikelihood"/>

			<!-- END Discrete Traits Model                                               -->

			
		</log>

		<!-- START Discrete Traits Model                                             -->
		<log id="fixed_tree_epoch.locationrateMatrixLog" logEvery="1000" fileName="fixed_tree_epoch.location.rates.log">
			<parameter idref="location.rates"/>
		</log>

		<!-- END Discrete Traits Model                                               -->


		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="500" nexusFormat="true" newickFormat="false" fileName="fixed_tree_epoch.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
<!--			<trait name="rate" tag="default.rate">
				<strictClockBranchRates idref="default.branchRates"/>
			</trait> -->
			<joint idref="joint"/>

			<!-- START Ancestral state reconstruction                                    -->
			<trait name="location.states" tag="location">
				<ancestralTreeLikelihood idref="location.treeLikelihood"/>
			</trait>

			<!-- END Ancestral state reconstruction                                      -->

		</logTree>
	</mcmc>
	
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
	
</beast>
